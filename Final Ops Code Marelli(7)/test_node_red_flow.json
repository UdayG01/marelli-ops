        "id": "d4e5f6g7h8i9j0k1",
        "type": "function",
        "z": "f9a7b2c8d3e4f5a6",
        "name": "Prepare Django Message",
        "func": "// Create the message payload for Django\nconst imageId = msg.payload || \"DEFAULT_TEST_IMG_001\";\nconst timestamp = new Date().toISOString().replace(/[-:.]/g, '').slice(0, 15);\n\n// Create the message structure that Django expects\nmsg.payload = {\n    \"id\": `nodered_${timestamp}`,\n    \"message\": imageId\n};\n\nmsg.headers = {\n    \"Content-Type\": \"application/json\"\n};\n\nmsg.url = \"http://localhost:8000/api/ml/from-node-red/\";\nmsg.method = \"POST\";\n\n// Store original image ID for display\nmsg.original_image_id = imageId;\n\nnode.log(`Sending Image ID: ${imageId} to Django`);\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
